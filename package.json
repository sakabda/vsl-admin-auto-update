{
  "name": "vessel-reporting-suite-v",
  "version": "1.0.2",
  "type": "commonjs",
  "main": "app.js",
  "description": "Vessel Reporting Suite V",
  "author": {
    "name": "VSL Marine Technology Pvt. Ltd.",
    "email": "vslmarine@gmail.com"
  },
  "icon": "favicon.ico",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-prod": "ng build --prod",
    "start:electron": "ng build --prod --base-href ./ && electron .",
    "postinstall": "patch-package",
    "dist-64": "electron-builder -w",
    "dist-32": "electron-builder -w --ia32",
    "dist-lin": "electron-builder -l",
    "pack": "build --dir"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.1.4",
    "@angular/common": "~12.1.4",
    "@angular/compiler": "~12.1.4",
    "@angular/core": "~12.1.4",
    "@angular/forms": "~12.1.4",
    "@angular/platform-browser": "~12.1.4",
    "@angular/platform-browser-dynamic": "~12.1.4",
    "@angular/router": "~12.1.4",
    "@types/electron": "^1.6.10",
    "@types/pouchdb": "^6.4.0",
    "electron-log": "^4.4.8",
    "electron-updater": "^5.3.0",
    "faker": "^6.6.6",
    "moment": "^2.29.1",
    "ng-connection-service": "^1.0.4",
    "ng-zorro-antd": "^12.0.1",
    "ng2-ckeditor": "^1.3.1",
    "ngx-electron": "^2.2.0",
    "patch-package": "^6.4.7",
    "pdfmake": "^0.2.5",
    "pouchdb": "^7.3.0",
    "pouchdb-find": "^7.3.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "uuid": "^8.3.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.1.4",
    "@angular/cli": "~12.1.4",
    "@angular/compiler-cli": "~12.1.4",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "@types/pdfmake": "^0.1.21",
    "@types/uuid": "^8.3.4",
    "codelyzer": "^6.0.0",
    "electron": "^19.0.7",
    "electron-builder": "^23.1.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.3.5"
  },
  "build": {
    "appId": "com.vslmarine.reporting",
    "icon": "favicon.ico",
    "win": {
      "target": "portable"
    },
    "mac": {
      "target": "pkg"
    },
    "portable": {
      "artifactName": "vsl-reporting-suite.exe"
    },
    "directories": {
      "output": "electron/output",
      "buildResources": "dist"
    },
    "files": [
      "**/*",
      "dist/**/*"
    ]
  }
}
